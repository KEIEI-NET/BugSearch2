        <wellknown type="Broadleaf.Application.Remoting.StockCommonInitUserDB,SFSIR02940R"  objectUri="MyAppStockCommonInitUser" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.StockCommonInitOfferDB,SFSIR02945R"  objectUri="MyAppStockCommonInitOffer" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.PaymentReadDB,SFSIR02105R"  objectUri="MyAppPaymentRead" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.ShrKingetDB,SFSIR02907R"  objectUri="MyAppShrKinget" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.CAddUpSMngGetInfDB,SFUKK01331R"  objectUri="MyAppCAddUpSMng" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.PaymentSlpDB,SFSIR02137R"  objectUri="MyAppPaymentSlp" mode="Singleton" />
        <wellknown type="Broadleaf.Application.Remoting.UpdateSuplAccPayPayRec,SFSIR02909R"  objectUri="MyAppUpdateSuplAccPayPayRec" mode="Singleton" />
