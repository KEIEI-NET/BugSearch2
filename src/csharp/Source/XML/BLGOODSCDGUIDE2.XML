<?xml version="1.0" encoding="UTF-8"?>
<definfo>
	<!-- ガイドフォームの設定  -->
	<GuideDef>
		<FormTitle>BL商品コードガイド</FormTitle>
		<FormMassge>BL商品コードを選択してください</FormMassge>
		<FormMassgeFontColor>MediumSpringGreen</FormMassgeFontColor>
		<FormHeight>635</FormHeight>
		<FormWidth>400</FormWidth>
		<StyleMode>new</StyleMode>
		<StartPosition>CenterParent</StartPosition>
		<StartPositionTop>0</StartPositionTop>
		<StartPositionLeft>0</StartPositionLeft>
		<ViewerSW>
			<SWName>通常表示</SWName>
			<SWParam>0</SWParam>
		</ViewerSW>
		<ViewerSW>
			<SWName>全メーカー表示</SWName>
			<SWParam>10</SWParam>
		</ViewerSW>
	</GuideDef>
	<!-- 選択対象データViewerの設定  -->
	<ViewerDef>
		<ViewerInfo>
			<ColName>BLコード</ColName>
			<ColKey>BLGoodsCode</ColKey>
			<ColWith>120</ColWith>
			<ColDataType>int</ColDataType>
		</ViewerInfo>		 
		<ViewerInfo>
			<ColName>枝番</ColName>
			<ColKey>BLGoodsCdDerivedNo</ColKey>
			<ColWith>120</ColWith>
			<ColDataType>int</ColDataType>
		</ViewerInfo>		 
		<ViewerInfo>
			<ColName>BL名称</ColName>
			<ColKey>BLGoodsHalfName</ColKey>
			<ColWith>250</ColWith>
			<ColDataType>string</ColDataType>
		</ViewerInfo>		 
		<ViewerInfo>
			<ColName>提供区分</ColName>
			<ColKey>DivisionName</ColKey>
			<ColWith>250</ColWith>
			<ColDataType>string</ColDataType>
		</ViewerInfo>		 
	</ViewerDef>
	<!-- 選択対象データの検索条件設定  -->
	<SearchDef>
		<SerchName>Broadleaf.Application.Controller.GoodsAcs</SerchName>
		<SerchAssm>MAKHN04112A.DLL</SerchAssm>
		<SerchInfoRoot>
			<SerchInfo type="string">EnterpriseCode</SerchInfo>
		</SerchInfoRoot>
	</SearchDef>
	<!-- 選択結果の設定  -->
	<SelectedInfoDef>
		<SelectedInfo type="string">EnterpriseCode</SelectedInfo>
		<SelectedInfo type="int">BLGoodsCode</SelectedInfo>
		<SelectedInfo type="int">BLGoodsCdDerivedNo</SelectedInfo>
		<SelectedInfo type="string">BLGoodsFullName</SelectedInfo>
		<SelectedInfo type="string">BLGoodsHalfName</SelectedInfo>
		<SelectedInfo type="int">BLGoodsGenreCode</SelectedInfo>
		<SelectedInfo type="string">LargeGoodsGanreCode</SelectedInfo>
		<SelectedInfo type="string">LargeGoodsGanreName</SelectedInfo>
		<SelectedInfo type="string">MediumGoodsGanreCode</SelectedInfo>
		<SelectedInfo type="string">MediumGoodsGanreName</SelectedInfo>
		<SelectedInfo type="string">DetailGoodsGanreCode</SelectedInfo>
		<SelectedInfo type="string">DetailGoodsGanreName</SelectedInfo>
		<SelectedInfo type="int">Division</SelectedInfo>
		<SelectedInfo type="string">DivisionName</SelectedInfo>
	</SelectedInfoDef>
</definfo>
